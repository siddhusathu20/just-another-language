println("ax^2 + bx + c")
let a = input("a: ")
let b = input("b: ")
let c = input("c: ")

let d = (numof b)*(numof b) - 4*(numof a)*(numof c)
if d == 0 then {
    println("The roots are real and equal")
} else if d > 0 then {
    println("The roots are real but not equal")
} else {
    println("The roots are complex")
}